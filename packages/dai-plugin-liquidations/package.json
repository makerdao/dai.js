{
  "name": "@makerdao/dai-plugin-liquidations",
  "description": "Plugin to add liquidations to dai.js",
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "unpkg": "umd/index.js",
  "jsdelivr": "umd/index.js",
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:umd",
    "build:umd": "webpack --config ../../webpack.config.js",
    "build:cjs": "../../scripts/build.sh",
    "clean": "rm -rf ./dist ./umd",
    "prepublishOnly": "yarn build",
    "testchain": "../../scripts/run-testchain.sh",
    "coverage": "yarn test --coverage",
    "test": "yarn testchain --ci jest --runInBand"
  },
  "peerDependencies": {
    "@makerdao/dai": ">0.17.0"
  },
  "dependencies": {
    "@makerdao/currency": "0.9.9",
    "@makerdao/services-core": "^0.9.9",
    "bignumber.js": "^8.1.1",
    "ramda": "^0.27.1"
  }
}
