{
  "name": "@makerdao/makerdao-exchange-integration",
  "version": "0.1.0",
  "contributors": [
    "Wouter Kampmann <wouter@makerdao.com>",
    "Sean Brennan <sean@makerdao.com>",
    "Tyler Sorensen <tyler@makerdao.com>",
    "Ethan Bennett <ethan@makerdao.com>"
  ],
  "description": "MakerDAO exchange integration library.",
  "license": "MIT",
  "keywords": [
    "makerdao",
    "mkr",
    "dai",
    "cdp",
    "eth",
    "margin"
  ],
  "bugs": {
    "url": "https://github.com/wkampmann/makerdao-integration-poc/issues",
    "email": "wouter@makerdao.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wkampmann/makerdao-integration-poc.git"
  },
  "module": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "build:frontend": "webpack --env=prod --progress --profile --colors",
    "build:backend": "./scripts/build-backend.sh",
    "coverage": "./with-deployed-system --ci jest --runInBand --coverage",
    "lint": "eslint web src test",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "prettier": "prettier --write --single-quote '{src,web,test,contracts}/**/*.js'",
    "start": "webpack-dev-server --config conf/webpack.dev.js",
    "test": "./with-deployed-system --ci jest --runInBand",
    "test:watch": "./with-deployed-system jest --watch --runInBand",
    "test:logs": "tail -f ganache.out | grep -v 'eth_blockNumber'",
    "test:net": "./with-deployed-system"
  },
  "lint-staged": {
    "./{src,web,test,contracts}/**/*.js": [
      "prettier --single-quote --write",
      "git add",
      "eslint web src test"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "ganache-cli": "^6.0.3",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "sass-loader": "^6.0.7",
    "solc": "^0.4.23",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue": "^2.5.16",
    "vue-loader": "^15.0.9",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "0x.js": "^0.37.2",
    "@0xproject/connect": "^0.6.12",
    "@0xproject/subproviders": "^0.10.1",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^7.2.1",
    "bunyan": "^1.8.12",
    "debug": "^3.1.0",
    "ethereumjs-util": "^5.2.0",
    "ethers": "^3.0.15",
    "ethers-web3-bridge": "0.0.1",
    "eventemitter2": "^5.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "lodash.uniq": "^4.5.0",
    "lodash.values": "^4.3.0",
    "toposort": "^2.0.2",
    "web3": "^0.20.6",
    "web3-provider-engine": "14.0.4"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "globalSetup": "<rootDir>/test/setup-global.js",
    "roots": [
      "src",
      "test"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup-test.js"
  }
}
